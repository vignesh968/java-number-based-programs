import java.util.*;  
import java.lang.Math;
class Main{
    public static void main(String []ags){
 Scanner s=new Scanner(System.in);
 int a=s.nextInt();
int temp=a;    
int i=0,k=0,l=0;
      while(a!=0)
    {  a=a/10;
       i++;
      
    }
    a=temp;
    while(a!=0)
    {
        k=a%10;
        l+=(int)Math.pow(k,i);
        a=a/10;
    }
	if(temp==l)
		System.out.println("number is armstrong");
	else
		System.out.println("not a armstrong");
	    
    }
} 

