import java.util.*;  
class Main {
    public static void main(String ...a){
   
    Scanner sc = new Scanner(System.in);
    System.out.println("Please give a number: ");
    int i = sc.nextInt(); 
     int res=0;
	while(i!=0){        
		res = res*10 + i%10;        
		i=i/10;    
	}    
	System.out.println("Number after reverse : "+res);    
    }
} 
//
1234
4321
