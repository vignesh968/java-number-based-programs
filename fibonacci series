import java.util.*;  
class Main{
    public static void main(String ...a){
    int first = 0, second = 1, result, i;
    Scanner sc= new Scanner(System.in);
    System.out.print("Enter number- ");  
    int n= sc.nextInt();  
    System.out.println("fibonacci series is: ");
   int res=0,f=0,l=1;
   for(int k=0;k<n;k++)
   {
    if(k<=1)
    {
        res=k;
    }
    else{
        res=f+l;
        f=l;
        l=res;
    }
    System.out.println(res);
   }
     
  }
} 
